{
  "compilerOptions": {
    "composite": true,
    "target": "es2016",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "paths": {
      "@/*": ["./src/*"],
      "vue-fields-ms": ["./src/main.ts"],
      "ckeditor": ["./ckeditor5/ckeditor.js"],
      "@scss/": ["./scss/*"]
    }
  },
  "include": ["src/**/*", "src/**/*.vue", "demo/**/*", "demo/**/*.vue"],
  "exclude": ["src/**/__tests__/*"]
}
